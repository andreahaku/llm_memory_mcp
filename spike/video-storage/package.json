{
  "name": "video-storage-spike",
  "version": "1.0.0",
  "description": "Spike implementation for video-based memory storage using QR codes",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node --esm src/index.ts",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "benchmark": "tsx benchmarks/performance.ts"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@ffmpeg/ffmpeg": "^0.12.10",
    "@ffmpeg/util": "^0.12.1",
    "@sec-ant/zxing-wasm": "^1.0.2",
    "canvas": "^2.11.2",
    "qrcode-generator": "^1.4.4",
    "zstandard": "^0.1.3"
  },
  "devDependencies": {
    "@types/node": "^20.14.15",
    "@vitest/ui": "^2.0.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5",
    "typescript": "^5.5.4",
    "vitest": "^2.0.5"
  },
  "keywords": [
    "video",
    "storage",
    "qr-codes",
    "memory",
    "compression",
    "spike"
  ],
  "author": "",
  "license": "MIT"
}